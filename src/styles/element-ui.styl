 //覆盖一些element-ui样式 覆盖css样式可在这里添加
.el-upload
  input[type="file"]
    display none !important


.el-select
  width 190px
.el-upload__input
  display none


//暂时性解决diolag 问题 https://github.com/ElemeFE/element/issues/2461
.el-dialog
  transform none
  left 0
  position relative
  margin 0 auto


//element ui upload
.upload-container
  .el-upload
    width 100%
    .el-upload-dragger
      width 100%
      height 200px



.el-icon-arrow-down
  margin-right 5px
  margin-top -5px


.el-menu--horizontal.el-menu--dark .el-submenu .el-menu-item.is-active,.el-menu-item.is-active
  color #fff!important
  background-color #32aab7!important
.el-menu--dark .el-menu-item:hover, .el-menu--dark .el-submenu__title:hover
  background-color #37c3d2!important
  color #fff!important

.el-menu--horizontal.el-menu--dark .el-submenu .el-menu-item.is-active,.el-menu-item.is-active:before
  content ""
  width 4px
  height 100%
  background #37c3d2
  position absolute
  left 0
.el-button
  transition all .5s!important
  border-radius 50px
.el-checkbox__input.is-checked .el-checkbox__inner
  background-color #32aab7
  border-color #32aab7
.el-checkbox__inner:hover
  border-color #32aab7
.el-checkbox__input.is-focus .el-checkbox__inner,.el-input__inner:focus
  border-color #32aab7
.el-button--primary:focus, .el-button--primary:hover
  background-color #3e5171
.el-checkbox__inner
  width 17px
  height 17px
.el-pager li.active
  background-color #324157
  border 1px solid #324157

.el-table__footer-wrapper thead div, .el-table__header-wrapper thead div,.el-table th,.el-table__fixed-header-wrapper thead div
  background-color #fff!important
.el-table__body tr.current-row>td,.el-table--enable-row-hover .el-table__body tr:hover>td
  background-color #f1f1f1!important
.icms-dialog
  .el-dialog__header
    background-color #5a6677
    padding 15px
    color #fff
    font-size 14px
    .el-icon-close
      color #fff
      font-size 12px
  .icms-dialog-content
    border 1px solid #5a6677
    padding 40px 25px 25px 25px
  nav
    background-color #5a6677
    color #fff
    padding 5px 10px
    top -14px
    z-index 999
    left 15px
.el-input__inner,.el-textarea__inner
  border 1px solid #798da5
.el-input--mini .el-input__inner
  height 25px
.el-input__inner,.el-input--small .el-input__inner
  height 32px
.icms-dialog
  border-radius 5px
  overflow hidden
.el-table th>.cell
  padding 5px 5px 15px 5px
.btn-orange
  background-color #ff885a!important
  border 1px solid #ff885a!important
  padding 9px 20px!important
  &:hover,&:focus
    opacity 0.7
    background-color #ff885a!important
    border 1px solid #ff885a!important
.btn-red
  background-color #ff7373!important
  border 1px solid #ff7373!important
  padding 9px 20px!important
  &:hover,&:focus
    opacity 0.7
    background-color #ff7373!important
    border 1px solid #ff7373!important
.btn-blue
  background-color #32aab7!important
  border 1px solid #32aab7!important
  padding 9px 20px!important
  &:hover,&:focus
    opacity 0.7
    background-color #32aab7!important
    border 1px solid #32aab7!important
.btn-green
  background-color #1acab1!important
  border 1px solid #1acab1!important
  padding 9px 20px!important
  &:hover,&:focus
    opacity 0.7
    background-color #1acab1!important
    border 1px solid #1acab1!important
.btn-white
  padding 10px 20px!important
  &:hover,&:focus
    opacity 0.7
.el-checkbox__input.is-indeterminate .el-checkbox__inner
  background-color #485b7b
  border-color #485b7b
.el-pagination
  .btn-prev
    border-radius 20px!important
    padding 5px 10px
    margin 0 5px
    border 1px solid #ccc
    .el-icon-arrow-left:before
      content "上一页"
      color #666
  .btn-next
    border-radius 20px!important
    padding 5px 10px
    margin 0 5px
    border 1px solid #ccc
    .el-icon-arrow-right:before
      content "下一页"
      color #666
.el-pager li
  border none
  border-radius 50%
  margin 0 5px
  border 1px solid #ccc
.el-pager li:last-child
  border none
  border 1px solid #ccc
.el-input--mini .el-input__inner
  height 30px
.el-table .cell, .el-table th>div
  padding 10px 5px!important
.el-table
  padding 15px 0!important
  border none!important
  border-radius 5px!important
  box-shadow 0px 1px 3px rgba(0, 0, 0, 0.12)!important
  .el-button+.el-button
    margin-left 3px
  thead
    border-bottom 1px solid #e6e6e6!important
  td,th.is-leaf
    border-bottom none!important
  .el-table__body-wrapper
    padding-top 10px
.el-picker-panel
  border-radius 4px
  box-shadow 0px 1px 3px rgba(0, 0, 0, 0.12)
.dropzone
  border-radius 4px
  border 1px solid #798da5
  background-color #f5f5f5
  &:hover
    background-color #efefef
  .dz-preview
    .dz-error-message
      top 120px!important
      left 80px!important
    .dz-details
      border-radius 4px
.el-table__fixed-right
  .el-table__fixed-header-wrapper
    padding-top 15px
  .el-table__fixed-body-wrapper
    padding
.roles-search
  padding 9px
  border 1px solid #dedede
  border-right none
  border-top-left-radius 50px
  border-bottom-left-radius 50px
  text-indent 1em
  width 200px

.header-search
  .el-form-item__content,.el-select
    width 240px!important
.el-date-editor.el-input,.el-select
  width 190px
.icms-dialog-content
  .el-date-editor.el-input,.el-select
    width 100%
.el-table::after, .el-table::before
  display none

.el-button--small,.el-loading-spinner .path,.el-loading-spinner .el-loading-text
  color #32aab7!important
  stroke #32aab7!important

.el-breadcrumb
  line-height 1!important
  margin-left 35px!important
  margin-top 35px!important
  font-size 13px!important
.el-date-table td.current:not(.disabled), .el-date-table td.end-date, .el-date-table td.start-date
  background-color #32aab7!important
  border-radius 50%
.el-date-table td.today:before
  border-top .5em solid #32aab7!important
.el-date-table td
  border-radius 50%
.el-date-table td.today,.el-button--text:focus, .el-button--text:hover,.el-select-dropdown.is-multiple .el-select-dropdown__item.selected,.el-time-panel__btn.confirm,.el-picker-panel__link-btn
  color #32aab7
.el-select .el-input__inner:focus,.el-textarea__inner:focus
  border-color #32aab7
.el-tag--primary
  background-color rgba(50, 170, 183, 0.2)
  border-color rgba(32,160,255,.2)
  color #32aab7
  .el-tag__close:hover
    background-color #32aab7
.el-date-picker.has-time
  width 260px!important
  min-width 260px!important
.el-time-panel__content::after, .el-time-panel__content::before
    background-color #32aab7
.el-loading-mask
  z-index 9
