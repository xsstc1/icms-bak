 //覆盖一些element-ui样式 覆盖css样式可在这里添加
.el-upload
  input[type="file"]
    display none !important



.el-upload__input
  display none


//暂时性解决diolag 问题 https://github.com/ElemeFE/element/issues/2461
.el-dialog
  transform none
  left 0
  position relative
  margin 0 auto


//element ui upload
.upload-container
  .el-upload
    width 100%
    .el-upload-dragger
      width 100%
      height 200px



.el-icon-arrow-down
  margin-right 5px
  margin-top -5px


.el-menu--horizontal.el-menu--dark .el-submenu .el-menu-item.is-active,.el-menu-item.is-active
  color #fff
  background-color #32aab7
.el-menu--dark .el-menu-item:hover, .el-menu--dark .el-submenu__title:hover
  background-color #37c3d2
  color #fff

.el-menu--horizontal.el-menu--dark .el-submenu .el-menu-item.is-active,.el-menu-item.is-active:before
  content ""
  width 4px
  height 100%
  background #37c3d2
  position absolute
  left 0
.el-button
  transition all .5s
.el-checkbox__input.is-checked .el-checkbox__inner
  background-color #485b7b
  border-color #485b7b
.el-checkbox__inner:hover
  border-color #485b7b
.el-checkbox__input.is-focus .el-checkbox__inner,.el-input__inner:focus
  border-color #485b7b
.el-button--primary:focus, .el-button--primary:hover
  background-color #3e5171
.el-checkbox__inner
  width 17px
  height 17px
.el-pager li.active
  background-color #324157
  border 1px solid #324157

.el-table__footer-wrapper thead div, .el-table__header-wrapper thead div,.el-table th,.el-table__fixed-header-wrapper thead div
  background-color #f5f5f5
.el-table__body tr.current-row>td,.el-table--enable-row-hover .el-table__body tr:hover>td
  background-color #f1f1f1
.icms-dialog
  .el-dialog__header
    background-color #5a6677
    padding 15px
    color #fff
    font-size 14px
    .el-icon-close
      color #fff
      font-size 12px
  .icms-dialog-content
    border 1px solid #5a6677
    padding 40px 20px 20px 20px
  nav
    background-color #5a6677
    color #fff
    padding 5px 10px
    top -14px
    z-index 999
    left 15px
.el-input__inner,.el-textarea__inner
  border 1px solid #798da5
.el-input--mini .el-input__inner
  height 25px
.icms-dialog
  border-radius 5px
  overflow hidden
.el-table th>.cell
  padding 0
.btn-orange
  background-color #ff885a
  border 1px solid #ff885a
  padding 8px 25px
.btn-red
  background-color #ff7373
  border 1px solid #ff7373
  padding 8px 25px
.btn-blue
  background-color #32aab7
  border 1px solid #32aab7
  padding 8px 25px
.btn-green
  background-color #65bb24
  border 1px solid #65bb24
  padding 8px 25px
.btn-white
  padding 8px 25px
.el-checkbox__input.is-indeterminate .el-checkbox__inner
  background-color #485b7b
  border-color #485b7b
